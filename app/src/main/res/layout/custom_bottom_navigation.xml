<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="@dimen/spacing_max_xxxl"
    android:background="@drawable/bg_bottom_navigation"
    tools:parentTag="androidx.constraintlayout.widget.ConstraintLayout">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/first_item"
        android:layout_width="@dimen/spacing_zero"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/second_item"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">

        <ImageView
            android:id="@+id/indicator"
            android:layout_width="@dimen/spacing_zero"
            android:layout_height="@dimen/spacing_zero"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:src="@drawable/bg_selected_navigation_bar"
            android:layout_marginHorizontal="@dimen/spacing_sl"
            android:layout_marginVertical="@dimen/spacing_minimum"
            tools:visibility="gone"/>

        <ImageView
            android:id="@+id/ic_first_item"
            android:layout_width="@dimen/spacing_big"
            android:layout_height="@dimen/spacing_big"
            android:layout_marginTop="@dimen/spacing_xs"
            android:scaleType="fitCenter"
            android:src="@drawable/ic_navigation_bar_home_icon"
            app:layout_constraintBottom_toTopOf="@+id/first_item_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <TextView
            android:id="@+id/first_item_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/ic_first_item"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:ellipsize="end"
            android:lines="1"
            android:text="Home"
            style="@style/Exo2_Regular_BottomNavigation"
            tools:visibility="visible"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/second_item"
        android:layout_width="@dimen/spacing_zero"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintStart_toEndOf="@+id/first_item"
        app:layout_constraintEnd_toStartOf="@+id/third_item"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">
        <ImageView
            android:id="@+id/indicator2"
            android:layout_width="@dimen/spacing_zero"
            android:layout_height="@dimen/spacing_zero"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:src="@drawable/bg_selected_navigation_bar"
            android:layout_marginHorizontal="@dimen/spacing_sl"
            android:layout_marginVertical="@dimen/spacing_minimum"
            tools:visibility="gone"/>
        <ImageView
            android:id="@+id/ic_second_item"
            android:layout_width="@dimen/spacing_big"
            android:layout_height="@dimen/spacing_big"
            android:scaleType="fitCenter"
            android:layout_marginTop="@dimen/spacing_xs"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/second_item_text"
            android:src="@drawable/ic_navigation_bar_category_icon"/>
        <TextView
            android:id="@+id/second_item_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/ic_second_item"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:ellipsize="end"
            android:lines="1"
            android:text="CatÃ¡logo"
            style="@style/Exo2_Regular_BottomNavigation"
            tools:visibility="visible"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/third_item"
        android:layout_width="@dimen/spacing_zero"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintStart_toEndOf="@id/second_item"
        app:layout_constraintEnd_toStartOf="@+id/fourth_item"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">
        <ImageView
            android:id="@+id/indicator3"
            android:layout_width="@dimen/spacing_zero"
            android:layout_height="@dimen/spacing_zero"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:src="@drawable/bg_selected_home_nav"
            android:layout_marginHorizontal="@dimen/spacing_sl"
            android:layout_marginVertical="@dimen/spacing_minimum"
            tools:visibility="gone"/>
        <ImageView
            android:id="@+id/ic_third_item"
            android:layout_width="@dimen/spacing_bl"
            android:layout_height="@dimen/spacing_xbl"
            android:scaleType="fitCenter"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:src="@drawable/ic_pink_logo"/>
        <ImageView
            android:id="@+id/ic_plus"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:scaleType="fitCenter"
            app:layout_constraintStart_toStartOf="@id/ic_third_item"
            android:layout_marginTop="@dimen/spacing_xs"
            android:layout_marginStart="@dimen/spacing_xml"
            app:layout_constraintTop_toTopOf="@id/ic_third_item"
            android:src="@drawable/ic_plus"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/fourth_item"
        android:layout_width="@dimen/spacing_zero"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintStart_toEndOf="@id/third_item"
        app:layout_constraintEnd_toStartOf="@+id/fifth_item"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">
        <ImageView
            android:id="@+id/indicator4"
            android:layout_width="@dimen/spacing_zero"
            android:layout_height="@dimen/spacing_zero"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:src="@drawable/bg_selected_navigation_bar"
            android:layout_marginHorizontal="@dimen/spacing_sl"
            android:layout_marginVertical="@dimen/spacing_minimum"
            tools:visibility="gone"/>
        <ImageView
            android:id="@+id/ic_fourth_item"
            android:layout_width="@dimen/spacing_big"
            android:layout_height="@dimen/spacing_big"
            android:scaleType="fitCenter"
            android:layout_marginTop="@dimen/spacing_xs"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/fourth_item_text"
            android:src="@drawable/ic_navigation_bar_favorite_icon"/>
        <TextView
            android:id="@+id/fourth_item_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/ic_fourth_item"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:text="Favoritos"
            style="@style/Exo2_Regular_BottomNavigation"
            android:ellipsize="end"
            android:lines="1"
            tools:visibility="visible"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/fifth_item"
        android:layout_width="@dimen/spacing_zero"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintStart_toEndOf="@id/fourth_item"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">
        <ImageView
            android:id="@+id/indicator5"
            android:layout_width="@dimen/spacing_zero"
            android:layout_height="@dimen/spacing_zero"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:src="@drawable/bg_selected_navigation_bar"
            android:layout_marginHorizontal="@dimen/spacing_sl"
            android:layout_marginVertical="@dimen/spacing_minimum"
            tools:visibility="gone"/>
        <ImageView
            android:id="@+id/ic_fifth_item"
            android:layout_width="@dimen/spacing_big"
            android:layout_height="@dimen/spacing_big"
            android:scaleType="fitCenter"
            android:layout_marginTop="@dimen/spacing_xs"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/fifth_item_text"
            android:src="@drawable/ic_navigation_bar_profile_icon"/>
        <TextView
            android:id="@+id/fifth_item_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/ic_fifth_item"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:text="Perfil"
            style="@style/Exo2_Regular_BottomNavigation"
            android:ellipsize="end"
            android:lines="1"
            tools:visibility="visible"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</merge>